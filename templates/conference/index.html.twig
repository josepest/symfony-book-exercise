{% extends 'base.html.twig' %}

{% block title %}
	Conference Guestbook
{% endblock %}

{% block body %}
	<h2>
		Give your feedback!
	</h2>
	{% for conference in conferences %}
		<h4>
			{{ conference }}
			|
			<a href="{{ path( 'conference-view', { slug: conference.slug } ) }}">
				View details
			</a>
		</h4>
	{% endfor %}
{% endblock %}
